{
	"name": "topshelfcraft/commerce-recurring-orders",
	"description": "Recurring Orders functionality for Craft Commerce 3.x",
	"type": "craft-plugin",
	"version": "3.0.0-beta.1",
	"license": "proprietary",
	"authors": [
		{
			"name": "Top Shelf Craft (Michael Rog)",
			"homepage": "https://topshelfcraft.com"
		},
		{
			"name": "Steadfast Design",
			"homepage": "https://steadfast.design"
		}
	],
	"support": {
		"issues": "https://github.com/TopShelfCraft/Commerce-Recurring-Orders/issues",
		"source": "https://github.com/TopShelfCraft/Commerce-Recurring-Orders"
	},
	"require": {
		"php": ">=7.1",
		"craftcms/commerce": "^3.2",
		"topshelfcraft/payment-source-tools-core": "^3.3"
	},
	"require-dev": {
		"codeception/codeception": "^4.1",
		"codeception/module-asserts": "^1.0.0",
		"codeception/module-phpbrowser": "^1.0.0",
		"codeception/module-yii2": "^1.1",
		"vlucas/phpdotenv": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"TopShelfCraft\\RecurringOrders\\": "src/"
		}
	},
	"extra": {
		"name": "Recurring Orders for Craft Commerce",
		"handle": "recurring-orders",
		"class": "TopShelfCraft\\RecurringOrders\\RecurringOrders"
	}
}
